<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Monster Encyclopedia</title>
    <script src="/node_modules/jquery/dist/jquery.js"></script>
    <script src="/node_modules/handlebars/dist/handlebars.js"></script>
    <script src="handlebars.js"></script>
  </head>
  <body>
      <h1>Monster Encyclopedia</h1>
    <script id="monster-template" type="text/x-handlebars-template">
     <li class="monsters-list">
         <!--<img src={{image}}/>-->
         <h2>name: {{name}}</h2>
         <p>location: {{location}}</p>
         <p>type: {{type}}</p>
         <p>description: {{description}}</p>
     </li>
    </script>
    <script id="all-monster-template" type="text/x-handlebars-template">
        <select class="all-monsters-list">
            {{#each monstersArray}}
                <option value="{{id}}">{{name}} from {{location}} </option>
            {{/each}}
        </select>
    </script>
  </body>
</html>
